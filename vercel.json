{
  "name": "echo-tube",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/swagger-ui.css",
      "dest": "/src/index.ts"
    },
    {
      "src": "/swagger-ui-bundle.js",
      "dest": "/src/index.ts"
    },
    {
      "src": "/swagger-ui-standalone-preset.js",
      "dest": "/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}